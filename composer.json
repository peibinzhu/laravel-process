{
  "name": "peibin/laravel-process",
  "description": "Peibin process component for Laravel.",
  "license": "MIT",
  "keywords": [
    "php",
    "laravel",
    "process"
  ],
  "authors": [
    {
      "name": "Peibin Zhu",
      "email": "zhupeibin1207@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "peibin/laravel-coordinator": "^1.0",
    "peibin/laravel-di": "^1.5",
    "peibin/laravel-swoole-event": "^1.0",
    "peibin/laravel-utils": "^1.5"
  },
  "autoload": {
    "psr-4": {
      "PeibinLaravel\\Process\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PeibinLaravelTest\\Process\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "PeibinLaravel\\Process\\ProcessServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true
}
